{
  "name": "novelsingletab",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "init": "git submodule update --init --remote",
    "setup": "lerna link --force-local && lerna bootstrap --force-local",
    "analysis": "hippy analysis",
    "start": "npm run build && hippy build --dev --custom-config ./webpack.config.js",
    "serve": "hippy-debug --verbose",
    "pack": "hippy build --custom-config webpack.config.js && hippy-zip ",
    "pack:android": "hippy build --android --custom-config webpack.config.js && hippy-zip ",
    "pack:ios": "hippy build --ios --custom-config webpack.config.js && hippy-zip ",
    "preupload": "npm run build",
    "upload": "npm run build && hippy build --custom-config webpack.config.js && hippy upload",
    "upload:android": "hippy build --android --custom-config webpack.config.js && hippy upload --android",
    "upload:ios": "hippy build --ios --custom-config webpack.config.js && hippy upload --ios ",
    "showcase": "out=$(npm run upload) && echo \"$out\" && showcase -c \"$out\" -n",
    "lint": "eslint src/ --ext .ts,.tsx --fix --quiet",
    "watch": "tsc --build tsconfig.json --watch",
    "build": "tsc --build tsconfig.json",
    "postinstall": "npm run build",
    "test": "jest -u --coverage",
    "test:local": "jest --coverage=0",
    "qcoverage": "qcoverage --lines=50 --diffLines=50 -s CZftLzClv+z+EZQbux8xjg=="
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "node node_modules/@commitlint/cli/lib/cli.js -E HUSKY_GIT_PARAMS",
      "pre-commit": "npm run lint && npm test && npm run qcoverage"
    }
  },
